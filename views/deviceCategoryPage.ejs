    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/mozillaOrAmazonPage';"> Mozilla/Amazon Phones </button>
    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/amazonTabletsPage';"> Amazon Tablets </button>
    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/windowsPage';"> Windows </button>
</div>
<div class="checkOut">
    <form id="checkOutForm" action="/" method="POST">
        <p class="swipeInstruction"> Please scan your device to enter it's ID for check out </p>
        <input type="text" class="form-control" id="checkOutIdInput" name="checkOutId" autofocus="true" style="height:35px;width:300px; left:410px; bottom:340px;position:fixed;" required="true">
        <button class="checkOutButton" onclick='getDeviceName("checkOutIdInput", "checkOutForm","true")' type="button"> Checkout Device </button>
    </form>
</div>
<div id="dialogoverlay"></div>
<div id="dialogbox">
    <div>
        <div id="dialogboxhead"></div>
