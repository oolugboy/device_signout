<div class="inner-cover">
    <h1 class="cover-heading">Search for available devices </h1>
    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/ipadsPage';"> Ipads </button>
    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/iPodsOrIPhonesPage';"> Ipods/Iphones </button>
    <button type="button" class="btn btn-lg btn-default-outline" onclick="location.href = '/androidPhonesPage';"> Android Phones </button>
