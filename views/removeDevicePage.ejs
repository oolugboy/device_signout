        </ul>
    </div>
    <!-- Now for the form -->
    <div class="container">
        <h3 class="optionsHeader"> Please enter the device name * </h3>
        <form id="adminForm">
            <div class="form-group">
                <input type="text" class="form-control tt-query" name="deviceName" id="deviceName" required="true" autocomplete="off" placeholder="Device Name">
                <button type="button" onclick='getDeviceName("deviceName","adminForm","remove")' class="submitButton">Remove device</button>
            </div>
        </form>
    </div>
    <div id="dialogoverlay"></div>
    <div id="dialogbox">
        <div>
            <div id="dialogboxhead"></div> 
            <div id="dialogboxfoot"></div>
        </div> 
    </div> 
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script>
/** Initialize the typeahead in a jQuery function **/
$(document).ready(function(){
        $('input.form-control').typeahead({
        name: 'deviceName',
        remote: { url:'/search?key=%QUERY' },
        limit: 5
    });
        });
</script>
