<!DOCTYPE html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
<meta name="description" content="">
<meta name="author" content="">
<link rel="icon" href="../../favicon.ico">
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<!-- Custom stylings --> 
<link rel="stylesheet" type="text/css" href="../styles/devCatStyle.css"> 
<link rel="stylesheet" href="../styles/dialog.css">
<link href="../styles/simpleSideBar.css" rel="stylesheet">
<script src="../../assets/js/ie-emulation-modes-warning.js"></script>
</head>

<body>
<div id="wrapper">
    <p class="welcomeMessage">
        Welcome <strong><%= user.username %> </strong>
    </p>
    <div class="logOutForm">
        <form method="get" action="/logout">
            <button class="logOutButton" type="submit" name="logOut"> Logout </button>
        </form>
    </div>
    <!-- Sidebar -->
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
            <li class="sidebar-brand">
            <p class="user"> Search Options</p>
            </li>
            <li>
            <a href="/ipadsPage">IPads</a>
            </li>
            <li>
            <a href='/iPodsOrIPhonesPage'> IPods/IPhones</a>
            </li>
            <li>
            <a href='/androidPhonesPage'> Android Phones</a>
            </li>
            <li>
            <a href='/androidTabletsPage'> Android Tablets </a>
            </li>
            <li>
            <a href='/mozillaOrAmazonPage'> Mozilla/Amazon Phones</a>
            </li>
            <li>
            <a href='/amazonTabletsPage'> Amazon Tablets</a>
            </li>
            <li>
            <a href='/windowsPage'> Windows </a>
            </li>
        </ul>
    </div>
    <div id="goAdmin" class="goAdmin">
        <button class="goAdmin" id="adminButton" onclick="location.href='/admin'"> Go Admin </button>
    </div> 
    <script src="/views/adminScript.js"></script>
    <script>
        adminHide("goAdmin", <%= admin %>); 
    </script>
    <div class="checkOut">
        <form id="checkOutForm" action="/" method="POST">
            <p class="swipeInstruction"> Please scan your device to enter it's ID for check out </p>
            <input type="text" class="form-control" id="checkOutIdInput" name="checkOutId" autofocus="true" required="true">
            <button class="checkOutButton" onclick='getDeviceName("checkOutIdInput", "checkOutForm","checkOut")' type="button"> Checkout Device </button>
        </form>
    </div>
    <div class="return">
        <form id="returnForm" action="/" method="POST">
            <p class="swipeInstruction"> Please scan your device to enter it's ID for return </p>
            <input type="text" class="form-control" id="returnIdInput" name="returnId" autofocus="true" required="true">
            <button class="checkOutButton" onclick='getDeviceName("returnIdInput","returnForm","return")' type="button"> Return Device </button>
        </form>
    </div>
   
    <div id="dialogoverlay"></div>
    <div id="dialogbox">
        <div>
            <div id="dialogboxhead"></div>
            <!--   <div id="dialogboxbody"></div> -->
            <div id="dialogboxfoot"></div>
        </div>
    </div>
</div>

<!-- Bootstrap core JavaScript
                            === === === === === === === === === === === === === === === === == -->
<!-- Placed at the end of the document so the pages load faster -->
<script src="/views/dialog.js"></script>  
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script>
window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')
</script>
<script src="../../dist/js/bootstrap.min.js"></script>
</body>
</html>
